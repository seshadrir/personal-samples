(define (cube x) (* x x x))
(define (square x) (* x x))
(define epsilon 0.000001)
(define (good-enough? guess x) (< (abs (- (cube guess) x)) epsilon))
(define (improv guess x) (/ (+ (* 2 guess) (/ x (square guess))) 3))
(define (cube-root x)
  (cube-iter 1 x)
  )
(define (cube-iter guess x)
  (if (good-enough? guess x)
      guess
      (cube-iter (improv guess x) x)
   )
  )
